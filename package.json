{
  "name": "sonofmagic",
  "version": "1.2.3",
  "description": "sonofmagic",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "yarn dts && cross-env NODE_ENV=development rollup -cw",
    "build": "yarn clean && yarn dts && cross-env NODE_ENV=production rollup -c",
    "build:tsc": "cross-env NODE_ENV=development tsc --build tsconfig.json",
    "dts": "tsc --emitDeclarationOnly -p tsconfig.dts.json",
    "test": "jest",
    "init:rename": "node scripts/init/rename.js",
    "init:bin": "node scripts/init/bin.js",
    "clean": "node scripts/clean.js",
    "raw": "cross-env TRACE=0 node bin/sonofmagic.js",
    "raw2": "cross-env GITHUB_CI=1 TRACE=0 node ./bin/sonofmagic.js",
    "raw:en": "cross-env TRACE=0 LANG=en node bin/sonofmagic.js",
    "fetch": "ts-node src/repos.ts",
    "raw:ts": "ts-node src/ts-cli-entry.ts",
    "raw:dic": "ts-node src/resources/dic.ts",
    "release": "bumpp",
    "sync": "cnpm sync sonofmagic yangqiming"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/yangqiming.git"
  },
  "files": [
    "dist",
    "bin",
    "assets"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "icebreaker",
    "sonofmagic",
    "yangqiming",
    "sonofmagic",
    "profile",
    "profile-icebreaker",
    "resume",
    "i18n"
  ],
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/yangqiming/issues"
  },
  "homepage": "https://github.com/sonofmagic/yangqiming#readme",
  "dependencies": {},
  "packageManager": "pnpm@8.6.2",
  "devDependencies": {
    "@icebreakers/eslint-config-ts": "^1.2.2",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.7",
    "@types/node-emoji": "^2.1.0",
    "@types/play-sound": "^1.1.0",
    "@types/prompts": "^2.4.4",
    "@types/qrcode": "^1.5.1",
    "@types/sound-play": "^1.1.0",
    "axios": "^1.4.0",
    "boxen": "^5.1.2",
    "builtin-modules": "^3.3.0",
    "bumpp": "^9.1.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "defu": "^6.1.2",
    "del": "^7.0.0",
    "enquirer": "^2.4.1",
    "eslint": "8.46.0",
    "i18next": "^23.4.1",
    "jest": "^29.6.2",
    "lodash": "^4.17.21",
    "node-emoji": "^2.1.0",
    "oo-ascii-tree": "^1.86.1",
    "ora": "^5.4.1",
    "os-locale": "^5.0.0",
    "play-sound": "^1.1.5",
    "prettier": "^3.0.1",
    "prompts": "^2.4.2",
    "qrcode": "^1.5.3",
    "rollup": "^3.27.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
  },
  "bin": {
    "yangqiming": "bin/yangqiming.js",
    "sonofmagic": "bin/sonofmagic.js"
  }
}
